select * from openquery(coop2,'select * from TMPTRAN order by trandate desc');



select * from openquery(coop2,'select * from TMPTRAN order by trandate desc');
select * from openquery(coop2,'select * from TMPTRAN where mbid=''45020692'' ');


select * from openquery(coop2, 'select * from member where mbid=''63112723'' ');


SELECT * FROM OPENQUERY(COOP2, 'SELECT * FROM TMPTRAN WHERE MBID=''45020692'' AND  EXTRACT(YEAR FROM TRANDATE)>=2019 ORDER BY TRANDATE DESC ')